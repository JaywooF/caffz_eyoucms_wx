.contact_img {
  width: 100%;
  height: 420rpx;
}

.contact_img image {
  width: 100%;
  height: 100%;
}

.first {
  padding-top: 30rpx;
  text-align:center;
  display: block;
}
.title_text {
  font-size: 15px;
  color: rgb(53, 53, 53);
  font-weight: bold;
  text-align: center;
}
.gap_lines {
  display: block;
  background-color: #ff7b00;
  width: 25px;
  height: 3px;
  text-align: center;
  margin: 10rpx auto;
  border-radius: 3px;
}


.contact_text {
display: flex;
padding: 15rpx 0;
align-items: center;
font-size: 26rpx;
color: #777;
border-bottom: 1px solid #f7f7f7;
padding-left: 15rpx;
position: relative;
}
.contact_text:last-child {
  border-bottom: none;
  }
.contact_text .ri {
margin-left: 20rpx;
position: absolute;
right: 30rpx;
}

.contact_text .le {
max-width: 520rpx;
margin-left: 10rpx;
}

.contact_text image {
width: 40rpx;
height: 40rpx;
}

.block-title {
  display: flex;
  padding: 30rpx;
  border-bottom: 1px solid #eee;
  background-color: #fff;
}
.title-box {
  margin: 0 auto;
  display: block;
  width: auto;
  font-size: 30rpx;
  z-index: 2;
  align-items: center;
  justify-content: space-between;
  flex-direction: row;
}
.title-box .title-leftline, .title-box .title-rightline {
  display: inline-block;
  width: 25rpx;
  height: 2rpx;
  background-color: #d9d9d9;
  vertical-align: middle;
  position: relative;
}
.title-box .title-txt {
  font-size: 15px;
  color: #000000;
  display: inline-block;
  vertical-align: middle;
  max-width: 500rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  padding: 0 20rpx;
}