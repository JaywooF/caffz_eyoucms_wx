/*
 * 上传图片按钮
*/
.page-img {
  font-size: 30rpx;
  color: #33373E;
  margin-bottom: 30rpx;
}
.page-img .imglist {
  display: flex;
  flex-wrap: wrap;
}
.page-img .imglist .item {
  width: 150rpx;
  height: 150rpx;
  margin-right: 22rpx;
  margin-bottom: 10rpx;
  position: relative;
}
.page-img .imglist .item image {
  width: 100%;
  height: 100%;
}
.page-img .imglist .item .delete {
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  top: -14rpx;
  right: -12rpx;
  border: 1px solid #e3e2e8;
  border-radius: 30rpx;
}
.page-img .imglist .item .delete .iconfont{
  position: absolute;
  top: -2rpx;
  left: 6rpx;
  font-size: 24rpx;
  color: #9e9e9e;
}
.page-img .imglist .last-item{
  width: 150rpx;
  height: 150rpx;
  line-height: 146rpx;
  text-align: center;
  border: 1rpx solid #e8e8e8;
}
.page-img .imglist .last-item .sign{
  color: #e8e8e8;
}

